public class NewC {
	
	public static void main(String args[]) {
		
		while(1==1) {
			Scanner in1 = new Scanner(System.in);
			int i=in1.nextInt();
			if(i=1) {
				System.out.println("it's still going");
			}
			else {
				break;
			}
			
		}
		
		
		Cipher c1 = new Cipher();
		c1.setText();
		c1.setKey();
		c1.setCipher();
		c1.getCipher();

	}
	
}
