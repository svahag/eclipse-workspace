package newP;
import java.util.Scanner;
public class nnn {
	

	public static void main(String[] args) {
		

    	System.out.print("Please enter the text to encrypt:");
		Scanner inTxt = new Scanner(System.in);
		String txt=inTxt.nextLine();
		System.out.print("Enter key:");
		Scanner inKey = new Scanner(System.in);
		int key=inKey.nextInt();
		String alp = "abcdefghijklmnopqrstuvwxyz";
    	String scr = alp.substring(17)+alp.substring(0, 17);
    	//String txt = "london is the capital of great britain";
    	String result="";
    	char ch;
    	for(int i=0; i<txt.length();i++) {
    		int x=alp.indexOf(txt.charAt(i));
    		
    		if(x>=0) {
    			ch=scr.charAt(x);
    			String s = Character.toString(ch);
    			result+=s;
    		}
    		else {
    			result+=" ";
    		}
    	}
    	
    	System.out.print(result);
    	
    	
    	
    	
    	
    	
